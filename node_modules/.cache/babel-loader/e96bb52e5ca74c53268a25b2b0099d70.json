   };\n  },\n  methods: {\n    open(link) {\n      switch (link) {\n        case \"linkedin\":\n          window.open(this.linkedin, \"_blank\");\n          break;\n        case \"github\":\n          window.open(this.github, \"_blank\");\n          break;\n        case \"angellist\":\n          window.open(this.angellist, \"_blank\");\n          break;\n        case \"resume\":\n          window.open(this.resume, \"_blank\");\n          break;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.home-title {\n  font-size: 28px;\n  font-weight: 500;\n}\n.bg-groc{\n  background-color:#ecb234 !important;\n}\n.bg-blau{\n  background-color:#84a3dc !important;\n}\n.bg-verd{\n  background-color:#74af6f !important;\n}\nimg {\n  max-width: 80%;\n  margin-top: 1em;\n}\n\n@media only screen and (max-width: 580px) {\n  img {\n    width: 200px;\n    margin-top: 10px;\n    margin-bottom: 10px;\n  }\n}\n\n.fa {\n  font-size: 15px;\n}\n\n.btn {\n  border-color: #669db3ff;\n  color: #669db3ff;\n  cursor: pointer;\n}\n\n.btn:hover {\n  background-color: #669db3ff;\n  border-color: #669db3ff;\n  color: white;\n}\n\n.btn:focus {\n  background-color: #669db3ff;\n  border-color: #669db3ff;\n  color: white;\n}\n\n.btn:focus {\n  outline: none !important;\n}\n\np {\n  text-align: justify;\n  font-weight: 400;\n}\n\n/* LEAVES */\n</style>\n"],"sourceRoot":"src/components"}]}